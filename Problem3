package solutions;

import java.util.Scanner;

public class Problem3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();

		int terms = (a % 2 == 0) ? (a - 1) : a;

		int num = 1;
		for (int i = 0; i < terms; i++) {
			System.out.print(num + " ");
			num += 2;
		}
		sc.close();
	}
}
